{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      // 登录表单数据（双向绑定）\n      loginForm: {\n        username: '',\n        password: ''\n      },\n      // 表单的验证规则\n      loginFormRules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在 3 到 10 个字符',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '长度在 3 到 10 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    login() {\n      this.$refs.loginFormRef.validate(valid => {\n        if (!valid) return;\n        axios.post(\"http://api.doc.jiyou-tech.com/mock/3979/login\", this.loginForm);\n      });\n    },\n    //点击重置表单\n    resetLoginForm() {\n      // console.log(this);\n      this.$refs.loginFormRef.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["axios","data","loginForm","username","password","loginFormRules","required","message","trigger","min","max","methods","login","$refs","loginFormRef","validate","valid","post","resetLoginForm","resetFields"],"sources":["src/views/LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"login_container\">\r\n    <div class=\"login_box\">\r\n      <!-- 头像区域 -->\r\n      <div class=\"avatar_box\">\r\n        <img src=\"../assets/logo.png\" alt=\"\" />\r\n      </div>\r\n      <!-- 登录表单 -->\r\n      <el-form ref=\"loginFormRef\" :model=\"loginForm\" :rules=\"loginFormRules\" label-width=\"0\" class=\"login_form\">\r\n        <!-- 用户名 -->\r\n        <el-form-item prop=\"username\">\r\n          <el-input v-model=\"loginForm.username\" prefix-icon=\"el-icon-user\"></el-input>\r\n        </el-form-item>\r\n        <!-- 密码 -->\r\n        <el-form-item prop=\"password\">\r\n          <el-input type=\"password\" v-model=\"loginForm.password\" prefix-icon=\"el-icon-lock\"></el-input>\r\n        </el-form-item>\r\n        <!-- 按钮 -->\r\n        <el-form-item>\r\n          <el-button type=\"primary\" class=\"btn\" @click=\"login\">登录</el-button>\r\n          <el-button type=\"info\" @click=\"resetLoginForm\">取消</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n        // 登录表单数据（双向绑定）\r\n        loginForm:{\r\n            username:'',\r\n            password:''\r\n        },\r\n        // 表单的验证规则\r\n        loginFormRules:{\r\n            username:[\r\n                { required: true, message: '请输入用户名', trigger: 'blur' },\r\n                { min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur' }\r\n            ],\r\n            password:[\r\n                { required: true, message: '请输入密码', trigger: 'blur' },\r\n                { min: 6, max: 15, message: '长度在 3 到 10 个字符', trigger: 'blur' }\r\n            ]\r\n        }\r\n    };\r\n  },\r\n  methods: {\r\n    login(){\r\n        this.$refs.loginFormRef.validate((valid)=>{\r\n            if(!valid) return;\r\n            axios.post(\"http://api.doc.jiyou-tech.com/mock/3979/login\",this.loginForm)\r\n         })\r\n    },\r\n    //点击重置表单\r\n    resetLoginForm(){\r\n        // console.log(this);\r\n        this.$refs.loginFormRef.resetFields();\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.login_container {\r\n  //   background-color: #2b4b6b;\r\n  background: url(\"../static/img/bg.jpeg\");\r\n  background-size: cover;\r\n  height: 100%;\r\n}\r\n.login_box {\r\n  width: 450px;\r\n  height: 300px;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n\r\n  .avatar_box {\r\n    height: 130px;\r\n    width: 130px;\r\